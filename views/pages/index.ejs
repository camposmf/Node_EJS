<!DOCTYPE html>
<html lang="pt-br">
<%- include("../partials/head"); %>
<body>
    <div class="container">

        <%- include("../partials/header"); %>

        <main>
            <div class="jumbotron">
                <h1>Node com EJS</h1>
                <p class="lead mt-4">Vamos aprender a trabalhar com arquivos EJS</p>
                <p class="lead mt-4"> <%= subTitle  %> </p>
            </div>

            <ul>

                <% qualities.forEach((quality) => { %>
                    <li class="nav-link">
                        <strong><%= quality.title %></strong> <%= quality.message %>
                    </li>
                <% }) %>

                
            </ul>
        </main>

        <%- include("../partials/footer"); %>
    </div>
</body>
</html>